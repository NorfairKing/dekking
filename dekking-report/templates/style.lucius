.linenum {
  display: inline;
}
progress[value] {
  /* Reset the default appearance */
  -webkit-appearance: none;
     -moz-appearance: none;
          appearance: none;

  accent-color: #{coveredCoveredColour};

  width: 250px;
  height: 20px;
}
::-webkit-progress-value {
  background-color: #{coveredCoveredColour};
}

::-moz-progress-bar {
  background-color: #{coveredCoveredColour};
}
